<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity" android:importantForAccessibility="no" android:background="#4C73A7">

    <ImageView
            android:layout_width="416dp"
            android:layout_height="275dp" app:srcCompat="@drawable/sky" android:id="@+id/cielo"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:contentDescription="@string/app_name"
            app:layout_constraintHorizontal_bias="1.0"/>
    <ImageView
            android:layout_width="585dp"
            android:layout_height="1011dp" app:srcCompat="@drawable/bosque" android:id="@+id/bosque"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="16dp"
    />
    <View
            android:id="@+id/rectanguloBlanco"
            android:layout_width="417dp"
            android:layout_height="510dp"
            android:background="@drawable/bg_white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.937"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/tio" android:id="@+id/imageView6"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/rectanguloBlanco" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="25dp" android:layout_marginStart="25dp" android:layout_marginTop="20dp"
            app:layout_constraintVertical_bias="0.3"/>


</androidx.constraintlayout.widget.ConstraintLayout>

