<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.runnerwar.ui.chat.ChannelFragment" android:background="@color/colorPrimary">

    <ImageView
        android:id="@+id/cielo"
        android:layout_width="958dp"
        android:layout_height="570dp"
        android:contentDescription="@string/app_name"
        app:layout_constraintBottom_toTopOf="@+id/rectanguloBlanco"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.756"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.675"
        app:srcCompat="@drawable/sky" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="500dp"
        android:src="@drawable/pj_cuenta"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.3" />

    <View
        android:id="@+id/rectanguloBlanco"
        android:layout_width="425dp"
        android:layout_height="453dp"
        android:layout_marginBottom="4dp"
        android:background="@drawable/bg_white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textEmailAddress"
            android:ems="10"
            android:id="@+id/reg_points"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:hint="Points"
            android:gravity="center"
            app:layout_constraintBottom_toBottomOf="@+id/rectanguloBlanco"
            app:layout_constraintVertical_bias="0.177"
            android:enabled="false" app:layout_constraintHorizontal_bias="0.436"
            app:layout_constraintTop_toBottomOf="@+id/reg_faction"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textEmailAddress"
            android:ems="10"
            android:id="@+id/reg_faction"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:hint="Faction"
            android:gravity="center"
            app:layout_constraintBottom_toBottomOf="@+id/rectanguloBlanco"
            app:layout_constraintVertical_bias="0.005"
            android:enabled="false" app:layout_constraintHorizontal_bias="0.436"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName4"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textEmailAddress"
            android:ems="10"
            android:id="@+id/reg_email"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:hint="Email"
            android:gravity="center"
            app:layout_constraintBottom_toBottomOf="@+id/rectanguloBlanco"
            app:layout_constraintVertical_bias="0.104"
            android:enabled="false" app:layout_constraintHorizontal_bias="0.436"
            app:layout_constraintTop_toBottomOf="@+id/barraSeparatoria"/>

    <ImageButton
        android:id="@+id/boton_edit"
        android:layout_width="27dp"
        android:layout_height="30dp"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="12dp"
        android:background="@drawable/boton_edit"
        app:layout_constraintBottom_toTopOf="@+id/reg_userName"
        app:layout_constraintStart_toEndOf="@+id/disk_save"
        app:layout_constraintTop_toTopOf="@+id/rectanguloBlanco"
        app:layout_constraintVertical_bias="0.941" />

    <ImageButton
        android:id="@+id/boton_eliminar"
        android:layout_width="29dp"
        android:layout_height="29dp"
        android:background="@drawable/boton_eliminar"
        app:layout_constraintBottom_toTopOf="@+id/reg_userName"
        app:layout_constraintEnd_toEndOf="@+id/rectanguloBlanco"
        app:layout_constraintHorizontal_bias="0.393"
        app:layout_constraintStart_toEndOf="@+id/boton_edit"
        app:layout_constraintTop_toTopOf="@+id/rectanguloBlanco"
        app:layout_constraintVertical_bias="0.566" />

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textEmailAddress"
            android:ems="10"
            android:id="@+id/reg_userName"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:hint="Name"
            android:gravity="center"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/rectanguloBlanco"
            app:layout_constraintTop_toTopOf="@+id/rectanguloBlanco" app:layout_constraintVertical_bias="0.144"
            android:enabled="false" app:layout_constraintHorizontal_bias="0.436"/>

    <ImageButton
        android:id="@+id/disk_save"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginStart="140dp"
        android:background="@drawable/floppy_disk__1_"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/reg_userName"
        app:layout_constraintStart_toEndOf="@+id/boton_logout"
        app:layout_constraintTop_toTopOf="@+id/rectanguloBlanco"
        app:layout_constraintVertical_bias="0.593" />

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Email"
            android:textStyle="bold"
            android:textSize="19dp"
            android:textColor="#000000"
            android:background="@null"
            android:ems="10"
            android:id="@+id/editTextTextPersonName3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.299" app:layout_constraintVertical_bias="0.07"
            app:layout_constraintBottom_toBottomOf="@+id/rectanguloBlanco"
            app:layout_constraintTop_toBottomOf="@+id/reg_userName"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Faction"
            android:textSize="19dp"
            android:textColor="#000000"
            android:background="@null"
            android:textStyle="bold"
            android:ems="10"
            android:id="@+id/editTextTextPersonName4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.299"
            app:layout_constraintVertical_bias="0.004" app:layout_constraintTop_toBottomOf="@+id/reg_email"
            app:layout_constraintBottom_toBottomOf="@+id/rectanguloBlanco"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Points"
            android:textSize="19dp"
            android:textColor="#000000"
            android:textStyle="bold"
            android:background="@null"
            android:ems="10"
            android:enabled="false"
            android:id="@+id/editTextTextPersonName5"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/reg_points"
            app:layout_constraintTop_toBottomOf="@+id/reg_faction"
            app:layout_constraintHorizontal_bias="0.299"
            app:layout_constraintVertical_bias="0.0"/>
    <ImageView
            android:src="@drawable/bg_line"
            android:layout_width="319dp"
            android:layout_height="13dp" android:id="@+id/barraSeparatoria"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/reg_userName"
            app:layout_constraintBottom_toTopOf="@+id/editTextTextPersonName3"/>

    <ImageButton
        android:id="@+id/boton_logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:background="@drawable/boton_logout"
        app:layout_constraintBottom_toBottomOf="@+id/disk_save"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/rectanguloBlanco"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/cambiarFaccion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cambiar faccion"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.175"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/reg_points"
        app:layout_constraintVertical_bias="0.357" />

    <Button
        android:id="@+id/boton_search"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Buscar usuarios"
        app:layout_constraintBottom_toBottomOf="@+id/rectanguloBlanco"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.282"
        app:layout_constraintStart_toEndOf="@+id/cambiarFaccion"
        app:layout_constraintTop_toBottomOf="@+id/reg_points"
        app:layout_constraintVertical_bias="0.38" />

    <View
        android:id="@+id/view"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:layout_marginTop="32dp"
        android:layout_marginBottom="300dp"
        android:background="@drawable/bg_white_1"
        app:layout_constraintBottom_toBottomOf="@+id/cielo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.058"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/reg_userCoins"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:text="Coins"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintHorizontal_bias="0.721"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toTopOf="@+id/view" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:src="@drawable/copa_del_pixel"
        app:layout_constraintBottom_toTopOf="@+id/rectanguloBlanco"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>
