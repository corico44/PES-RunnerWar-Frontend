<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
    tools:context=".ui.buscarCuenta.SearchFragment" android:importantForAccessibility="no">

        <TextView
            android:id="@+id/SearchUser"
            android:layout_width="300dp"
            android:layout_height="36dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="6dp"
            android:hint="Search user"
            app:layout_constraintEnd_toStartOf="@+id/boton_searchuser"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/boton_searchuser"
            android:layout_width="91dp"
            android:layout_height="37dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:text="SEARCH"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/UserRetornat"
            android:layout_width="358dp"
            android:layout_height="40dp"
            android:layout_marginTop="47dp"
            android:layout_marginEnd="37dp"
            android:text="TextView"
            android:visibility="invisible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/boton_searchuser" />

        <TextView
            android:id="@+id/FactionSUser"
            android:layout_width="355dp"
            android:layout_height="33dp"
            android:layout_marginTop="1dp"
            android:layout_marginEnd="38dp"
            android:text="TextView"
            android:visibility="invisible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/UserRetornat" />
</androidx.constraintlayout.widget.ConstraintLayout>
